    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "svelte": "^3.24.0",
    "svelte-check": "^0.1.51",
    "svelte-preprocess": "^4.0.7",
